    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pol.esquaixlligues"
    },
    "android": {
      "package": "com.pol.esquaixlligues"
    }
  }
}